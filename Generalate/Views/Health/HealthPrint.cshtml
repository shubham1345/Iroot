@model Generalate.Models.tbl_Health

@{
    Layout = null;
}
<head>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/bootstrap/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="~/bootstrap/css/bootstrap.min.css">

    <script type="text/javascript">
    $(document).ready(function () {

        $.getJSON("GetAll",
        function (json) {
            var tr;

            //Append each row to html table
            for (var i = 0; i < json.length; i++) {
                tr = $('<tr />');
                tr.append("<td hidden='hidden'>" + json[i].HealthId + "</td>");
                tr.append('<td><a href=ViewProfile/' + json[i].HealthId + '>' + json[i].Name + '</a></td>');
                tr.append("<td>" + json[i].SirName + "</td>");
                tr.append("<td>" + json[i].FromDate + "</td>");
                tr.append("<td>" + json[i].ToDate + "</td>");
                tr.append("<td>" + json[i].Complaint + "</td>");
                tr.append("<td>" + json[i].Doctor + "</td>");
                tr.append("<td>" + json[i].Diagnosis + "</td>");
                tr.append("<td>" + json[i].Hospital + "</td>");
                $('table').append(tr);

            }
            $('#EmpInfo').DataTable();
        });
    });

    </script>

</head>


    <div class="form-horizontal">

        <div class="container">
            <div class="form-horizontal">
                <br />
                <table class="table table-striped  table-hover" id="EmpInfo">
                    <thead>
                        <tr>
                            <th hidden="hidden"> Id </th>
                            <th> SurName </th>
                            <th> Name </th>
                            <th> FromDate </th>
                            <th> ToDate </th>
                            <th> Complaint </th>
                            <th> Doctor </th>
                            <th> Diagnosis </th>
                            <th> Hospital </th>
                        </tr>
                    </thead>
                    <tbody class="tbody"> </tbody>
                </table>
            </div>
            
        </div>

    </div>



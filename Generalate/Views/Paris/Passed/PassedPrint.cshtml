@model Generalate.Models.tbl_Passed

@{
    Layout = null;
}
<head>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/bootstrap/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="~/bootstrap/css/bootstrap.min.css">

    <script type="text/javascript">
    $(document).ready(function () {

        $.getJSON("GetAll",
        function (json) {
            var tr;

            //Append each row to html table
            for (var i = 0; i < json.length; i++) {
                tr = $('<tr />');
                tr.append("<td hidden='hidden'>" + json[i].PassedId + "</td>");
                tr.append('<td><a href=ViewProfile/' + json[i].PassedId + '>' + json[i].Name + '</a></td>');
                tr.append("<td>" + json[i].MemberID + "</td>");
                tr.append("<td>" + json[i].LastCommunity + "</td>");
                tr.append("<td>" + json[i].Cause + "</td>");
                tr.append("<td>" + json[i].Date + "</td>");
                tr.append("<td>" + json[i].Time + "</td>");
                tr.append("<td>" + json[i].InstitutionPlace + "</td>");
                //LastPlaceRites
                //ville
                //obituary
                $('table').append(tr);

            }
            $('#EmpInfo').DataTable();
        });
    });

    </script>

</head>


 

<div class="form-horizontal">
    <div class="container">
        <div class="form-horizontal">
            <br />
            <table class="table table-striped  table-hover" id="EmpInfo">
                <thead>
                    <tr>
                        <th hidden="hidden"> Id </th>
                        <th> Sur Name </th>
                        <th> MemberID </th>
                        <th> LastCommunity </th>
                        <th> Cause </th>
                        <th> Date </th>
                        <th> Time </th>
                        <th> InstitutionPlace </th>
                    </tr>
                </thead>
                <tbody class="tbody"> </tbody>
            </table>
        </div>
        
    </div>
</div>



@model Generalate.Models.tbl_Entry

@{
    Layout = null;
}
<head>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/bootstrap/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="~/bootstrap/css/bootstrap.min.css">

    <script type="text/javascript">
    $(document).ready(function () {

        $.getJSON("GetAll",
        function (json) {
            var tr;

            //Append each row to html table
            for (var i = 0; i < json.length; i++) {
                tr = $('<tr />');
                tr.append("<td hidden='hidden'>" + json[i].EntryId + "</td>");
                // tr.append('<td><a href=ViewProfile/' + json[i].EntryId + '>' + json[i].MemberID + '</a></td>');
                tr.append("<td>" + json[i].Name + "</td>");
                tr.append("<td>" + json[i].Spare1 + "</td>");
                tr.append("<td>" + json[i].DateOfBaptism + "</td>");
                tr.append("<td>" + json[i].ChurchName1 + "</td>");
                tr.append("<td>" + json[i].Minister1 + "</td>");
                tr.append("<td>" + json[i].Place1 + "</td>");
                tr.append("<td>" + json[i].DateOfBaptism + "</td>");
                tr.append("<td hidden='hidden'>" + json[i].ChurchName2 + "</td>");
                tr.append("<td hidden='hidden'>" + json[i].Minister2 + "</td>");
                tr.append("<td hidden='hidden'>" + json[i].Place2 + "</td>");
                $('table').append(tr);

            }
        });
        
    });

    </script>

</head>


<div class="form-horizontal">
    <div class="container">
        <div class="form-horizontal">
            <br />
                
            <table class="table table-striped  table-hover" id="EmpInfo">
                <thead>
                    <tr>
                        <th hidden="hidden"> Id </th>
                        <th> Name</th>
                        <th>SurName</th>
                        <th> DateOfBaptism </th>
                        <th> ChurchName </th>
                        <th> Minister </th>
                        <th> Place </th>
                        <th> DateOfConfirmation </th>
                        <th hidden="hidden"> ChurchName </th>
                        <th hidden="hidden"> Minister </th>
                        <th hidden="hidden"> Place </th>
                    </tr>
                </thead>
                <tbody class="tbody"> </tbody>
            </table>
        </div>
            
    </div>
</div>

            onSelect: function (selected) {
                $("#todate").datepicker("option", "minDate", selected)
            }
        });
        $('#todate').datepicker({
            changeMonth: true,
            changeYear: true,
            dateFormat: "dd/mm/yy",
            yearRange: '1600:2020',
            onSelect: function (selected) {
                $("#fromdate").datepicker("option", "maxDate", selected)
            }
        });
    </script>
    <script>
        $("#ArchiveDiv").css("display", "none");
        $('#chkArchive').click(function () {
            if ($(this).prop("checked") == true) {
                $("#ArchiveDiv").css("display", "block")
            }
            else if ($(this).prop("checked") == false) {
                $("#ArchiveDiv").css("display", "none")
            }
        });
        GetGrid();
        $("#SearchTxt").on('input', function () {
                GetGrid();

        });
        $(".btnscr").on('click', function () {
            $('.Membergrid').hide();

        });
        $("#previousData").on('click', function () {
            var firstItem = $("#StartingId").val();
            var lastItem = $("#LastId").val();

            if (firstItem != 1) {
                $("#StartingId").val(firstItem * 1 - 10);
                $("#LastId").val(lastItem * 1 - 10);
                    GetGrid();
            }

        });
        $("#nextData").on('click', function () {
            var firstItem = $("#StartingId").val();
            var lastItem = $("#LastId").val();

            var totalcount = $("#totalcount").text();
            if (lastItem <= totalcount) {
                $("#StartingId").val(firstItem * 1 + 10);
                $("#LastId").val(lastItem * 1 + 10);
                    GetGrid();
            }

        });
        $("#ExportMemberData").on('click', function () {
            var firstItem = $("#StartingId").val();


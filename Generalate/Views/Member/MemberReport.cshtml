            //alert(evt.target.id);
            if (evt.target.id == "mySidenav")
                return;
            else if (evt.target.className == "notdivhide" || evt.target.className == "notdivhide2") {
                return;
            }

            else if (document.getElementById("mySidenav").style.width == '250px') {
                $('#mySidenav').hide();
                closeNav();
                return;
            }
            else if (evt.target.id == "ExportGrid") {
                //$('#mySidenav_Dynamic2').toggle();
                //closeNavExp();
                return;
            }
            else if (document.getElementById("mySidenav_Dynamic2").style.width == '250px') {
                $('#mySidenav').hide();
                //$('#mySidenav_Dynamic2').toggle();
                closeNavExp();
                return;
            }
            else {
                //closeNav();
                return;
            }
            //For descendants of menu_content being clicked, remove this check if you do not want to put constraint on descendants.
            if ($(evt.target).closest('#menu_content').length)
                return;

            //Do processing of click event here for every element except with id menu_content

        });
        function GetGrid() {
            var firstItem = $("#StartingId").val();
            var lastItem = $("#LastId").val();
            var SearchTxt = $("#SearchTxt").val();
            var grp = $("#ProvinceDrop").val();
            //alert(firstItem);

            var FilterType = $('#isFilterPorC').val();
            var FilterData = $('#strFilterTypeData').val();
            //alert("kdnv");
            jQuery.ajax({
                url: '/Member/GetGrid_Member2',
                type: "POST",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify({ name: 'Deep', firstItem: firstItem, lastItem: lastItem, SearchTxt: SearchTxt, grp: grp, FilterType: FilterType, FilterData: FilterData }),
                success: function (result) {

                    $("#PartialTable").empty();
                    $("#PartialTable").html(result);
                    var countData = $('#DynamicTotalCount').val();
                    $("#totalcount").text(countData);
                    $('.notdivhide').prop("checked", true);
                    $('.hide-15').hide();


                },
                error: function (err) {
                    alert(err.statusText);
                }
            });
            @* alert('@ViewBag.totalCount');
            $("#totalcount").html(@ViewBag.totalCount);*@
        }
        function openNav() {
            document.getElementById("mySidenav").style.width = "250px";
            $('#mySidenav').show();
            //alert(document.getElementById("mySidenav").style.width);
        }
        function closeNav() {
            document.getElementById("mySidenav").style.width = "0";
            $('#mySidenav').hide();
            var ListType = $('#Dynamic_Checkbox').val();
            var checkedlist = '';
            $('input[class=notdivhide]').each(function () {
                var sThisVal = (this.checked ? $(this).val() : "");
                if (sThisVal == 'on') {
                    checkedlist += $(this).attr("id").replace('checkbox-grid-', '') + ',';
                }
            });
            //alert(checkedlist);
            var ListData = checkedlist.replace(/,\s*$/, "");
            jQuery.ajax({
                url: '/Member/SetDynamicColumn',
                type: "POST",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify({ ListType: ListType, ListData: ListData }),
                success: function (result) {

                },
                error: function (err) {
                    alert(err.statusText);
                }
            });
        }
        function closeNavExp() {
            document.getElementById("mySidenav_Dynamic2").style.width = "0";
            $('#mySidenav').hide();
            var ListType = 3;
            var checkedlist = '';
            $('input[class=notdivhide2]').each(function () {
                var sThisVal = (this.checked ? $(this).val() : "");
                if (sThisVal == 'on') {
                    checkedlist += $(this).attr("id").replace('checkbox-grid2-', '') + ',';
                }
            });

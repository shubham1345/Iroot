@model Generalate.Models.tbl_Jubilee

@{
    Layout = null;
}
<head>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/bootstrap/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="~/bootstrap/css/bootstrap.min.css">

    <script type="text/javascript">
    $(document).ready(function () {

        $.getJSON("GetAll",
        function (json) {
            var tr;

            //Append each row to html table
            for (var i = 0; i < json.length; i++) {
                tr = $('<tr />');
                tr.append("<td hidden='hidden'>" + json[i].JubileeID + "</td>");
                tr.append("<td>" + json[i].Name + "</td>");
                tr.append("<td>" + json[i].SirName + "</td>");
               // tr.append("<td>" + json[i].MemberID + "</td>");
                tr.append("<td>" + json[i].FirstProffession + "</td>");
                tr.append("<td>" + json[i].SilverJubilee + "</td>");
                tr.append("<td>" + json[i].GoldenJubilee + "</td>");
                tr.append("<td>" + json[i].DiamondJubilee + "</td>");
                $('table').append(tr);

            }
        });
    });

    </script>

</head>

<div class="form-horizontal">

    <div class="container">
        <div class="form-horizontal">
            <br />
            <table class="table table-striped  table-hover" id="EmpInfo">
                <thead>
                    <tr>
                        <th hidden="hidden"> Id </th>
                        <th> Name </th>
                        <th> SurName </th>
                        <th> FirstProffession </th>
                        <th> SilverJubilee </th>
                        <th> GoldenJubilee </th>
                        <th> DiamondJubilee </th>
                    </tr>
                </thead>
                <tbody class="tbody"> </tbody>
            </table>
        </div>

    </div>

</div>
}
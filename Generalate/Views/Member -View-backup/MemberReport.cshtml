@section scripts{
    <script src="~/js/printThis.js"></script>
    <script src="~/css/DropDownSelect2/dist/js/select2.min.js"></script>
    <script src="~/Scripts/additional/MemberReportJS.js"></script>
    <script>
        $('#btnPrint').on("click", function () {
            $('#prientbox').printThis();
        });
        $('#fromdate').datepicker({
            changeMonth: true,
            changeYear: true,
            dateFormat: "dd/mm/yy",
            yearRange: '1600:2020',
            onSelect: function (selected) {
                $("#todate").datepicker("option", "minDate", selected)
            }
        });
        $('#todate').datepicker({
            changeMonth: true,
            changeYear: true,
            dateFormat: "dd/mm/yy",
            yearRange: '1600:2020',
            onSelect: function (selected) {
                $("#fromdate").datepicker("option", "maxDate", selected)
            }
        });
    </script>
    <script>
        $("#ArchiveDiv").css("display", "none");
        $('#chkArchive').click(function () {
            if ($(this).prop("checked") == true) {
                $("#ArchiveDiv").css("display", "block")
            }
            else if ($(this).prop("checked") == false) {
                $("#ArchiveDiv").css("display", "none")
            }
        });

    </script>

}
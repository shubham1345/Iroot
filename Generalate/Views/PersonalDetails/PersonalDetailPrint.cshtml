@model Generalate.Models.tbl_PersonalDetails

@{
    Layout = null;
}
<head>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/bootstrap/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="~/bootstrap/css/bootstrap.min.css">

    <script type="text/javascript">
    $(document).ready(function () {

        $.getJSON("GetAll",
        function (json) {
            var tr;

            //Append each row to html table
            for (var i = 0; i < json.length; i++) {
                tr = $('<tr />');
                tr.append("<td hidden='hidden'>" + json[i].PersonalDetailsId + "</td>");
                tr.append("<td>" + json[i].Spare1 + "</td>");
                tr.append('<td><a href=ViewProfile/' + json[i].PersonalDetailsId + '>' + json[i].Name + '</a></td>');
                tr.append("<td>" + json[i].NameBaptismial + "</td>");
                tr.append("<td>" + json[i].SirName + "</td>");
                tr.append("<td>" + json[i].DOB + "</td>");
                tr.append("<td>" + json[i].FeastDays + "</td>");
                tr.append("<td>" + json[i].EmailID + "</td>");
                tr.append("<td>" + json[i].Mobile + "</td>");
                tr.append("<td>" + json[i].AadharNo + "</td>");
                $('table').append(tr);

            }
        });
    });

    </script>

</head>


<div class="form-horizontal">
    <div class="container">
        <div class="form-horizontal">
            <br />
            <table class="table table-striped  table-hover" id="EmpInfo">
                <thead>
                    <tr>
                        <th hidden="hidden"> Id </th>
                         <th> Sur Name </th>
                                            <th> Name </th>                                            
                                            <th>Baptismial Name</th>
                                            <th>Other Names</th>                                            
                                            <th> Date of Birth </th>
                                            <th> Feast Days </th>
                                            <th> Email ID </th>
                                            <th> Mobile Number </th>                                          
                                             <th> Aadhar Number </th>                                           
                                            <th> Action </th>
                    </tr>
                </thead>
                <tbody class="tbody"> </tbody>
            </table>
        </div>

    </div>


</div>


@{
    ViewBag.Title = "AllProvinceDataAdd";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<ul class="breadcrumb">
    <li><a href="#">Dashboard</a></li>
    
</ul>

<div class="page-content-wrap">
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-body">
                    <div class="container-fluid">                      
                        <div class="row">
                            <div class="col-md-12 col-sm-12 col-xs-12 jss_c">
                                @*<ul class="nav nav-tabs" id="myTab">
                                    <li class="active"><a data-toggle="tab" href="#dataitemappointment" class="sis_tab_a click">Appointment</a></li>                       
                                </ul>*@
                                <div class="tab-content tab_sis">
                                    <div id="dataitemappointment" class="tab-pane fade in active">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="panel panel-primary">
                                                    <div class="panel-heading" style="background-color:#29b2e1">All Province Detail</div>
                                                    <div class="panel-body">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="col-sm-12">
                                                               <button class="btn btn-danger pull-right"><a href="/AllProvinceDataAdd/AllProvinceDataView" class="button_a">View All</a></button>
                                                                </div>
                                                                <div class="box box-info">
                                                                    <div class="box-body">
                                                                        <div class="row">
                                                                            <form id="ProvinceForm" class="form-horizontal" action="/AllProvinceDataAdd/AddProvinceData" method="post" enctype="multipart/form-data">
                                                                                <div class="col-sm-12">
                                                                                    <div class="col-sm-6">
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">Province Name:</label>
                                                                                            <div class="col-sm-6">
                                                                                                <select type="dropdown" class="form-control" name="ProvinceName" id="ProvinceName">
                                                                                                    <option value="GrandTotal">Grand Total</option>
                                                                                                    @foreach (var item in @ViewBag.AllProvince)
                                                                                                    {
                                                                                                        <option value="@item.ProvinceName">@item.ProvinceName</option>
                                                                                                    }

                                                                                                </select>
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">Province Code:</label>
                                                                                            <div class="col-sm-6">
                                                                                                <select type="dropdown" class="form-control" name="ProvinceCode" id="ProvinceName">
                                                                                                    <option value="GrandTotal">Grand Total</option>
                                                                                                    @foreach (var item in @ViewBag.AllProvince)
                                                                                                    {                                                                                                       
                                                                                                        <option value="@item.Place">@item.Place</option>
                                                                                                    }

                                                                                                </select>
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">From Date</label>
                                                                                            <div class="col-sm-6">
                                                                                                <input type="text" class="form-control" name="StartDate" id="StartDate" readonly placeholder="Start Date">
                                                                                                <input type="hidden" id="PRId" name="PRId" class="form-control" />
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">To Date</label>
                                                                                            <div class="col-sm-6">
                                                                                                <input type="text" readonly class="form-control" name="EndDate" id="EndDate" placeholder="End Date">
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">Total Profession Jan.</label>
                                                                                            <div class="col-sm-6">
                                                                                                <input type="text" class="form-control" name="Totalproffess" id="Totalproffess">
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">Total Prof. And Nov Jan.</label>
                                                                                            <div class="col-sm-6">
                                                                                                <input type="text" class="form-control" name="GrandtotalProfNovStart" id="GrandtotalProfNovStart">
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">Temp. Or First Prof.</label>
                                                                                            <div class="col-sm-6">
                                                                                                <input type="text" class="form-control" name="TPorFP" id="TPorFP">
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">Perpetual Profession</label>
                                                                                            <div class="col-sm-6">
                                                                                                <input type="text" class="form-control" name="PerProf" id="PerProf">
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">Total Profession Dec.</label>
                                                                                            <div class="col-sm-6">
                                                                                                <input type="text" class="form-control" name="GrandtotalProfLast" id="GrandtotalProfLast">
                                                                                            </div>
                                                                                        </div>

                                                                                    </div>
                                                                                    <div class="col-sm-6">
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">Novices 1st Year</label>
                                                                                            <div class="col-sm-6">
                                                                                                <input type="text" class="form-control" name="FirststNovices" id="FirststNovices">
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">Novices 2nd Year</label>
                                                                                            <div class="col-sm-6">
                                                                                                <input type="text" class="form-control" name="SecondNovices" id="SecondNovices">
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">Total Prof. And Nov Dec.</label>
                                                                                            <div class="col-sm-6">
                                                                                                <input type="text" class="form-control" name="GrandtotalProfNovLast" id="GrandtotalProfNovLast">
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">Depart Novices</label>
                                                                                            <div class="col-sm-6">
                                                                                                <input type="text" class="form-control" name="NovDepart" id="NovDepart">
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">Depart VT</label>
                                                                                            <div class="col-sm-6">
                                                                                                <input type="text" class="form-control" name="VTDepart" id="VTDepart">
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">Depart VP</label>
                                                                                            <div class="col-sm-6">
                                                                                                <input type="text" class="form-control" name="VPDepart" id="VPDepart">
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">Death</label>
                                                                                            <div class="col-sm-6">
                                                                                                <input type="text" class="form-control" name="Death" id="Death">
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">Transfer(-)</label>
                                                                                            <div class="col-sm-6">
                                                                                                <input type="text" class="form-control" name="Transfer1" id="Transfer1">
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <label for="inputPassword3" class="col-sm-3 control-label">Transfer(+)</label>
                                                                                            <div class="col-sm-6">
                                                                                                <input type="text" class="form-control" name="Transfer2" id="Transfer2">
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-group">
                                                                                            <div class="col-sm-6">
                                                                                                <button type="submit" class="btn btn-info pull-right" id="btnappoSave">Save</button>

                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </form>
                                                                        </div>
                                                                    </div>


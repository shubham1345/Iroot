    <script>
        $('.CongreView').on("click", function () {
            $('#myModal2').modal('show');
            var id = $(this).attr("data-val");
            GetAllGeneralateById1(id);
        });
        function GetAllGeneralateById1(id) {
            $.ajax({
                url: "/IdGenerate/GetAllGeneralateById1?id=" + id,
                type: "GET",
                contentType: "application/json;charset=utf-8",
                dataType: "json",
                success: function (data) {
                    $("#GeneralateNameGeneralate").text(data[0].CongregationName);
                    $("#EstablishmentGeneralate").text(data[0].Establishment);
                    $("#EmailGeneralate").text(data[0].Email);
                    $("#WebsiteGeneralate").text(data[0].Website);
                    $("#PhoneGeneralate").text(data[0].Phone);
                    $("#FaxGeneralate").text(data[0].Fax);
                    $("#CountryGeneralate").text(data[0].Country);
                    $("#AddressGeneralate").text(data[0].Address);
                    $("#DioceseNameGeneralate").text(data[0].Diocese);
                    $("#ActivitiesGeneralate1").text(data[0].Activities);
                    $("#HistoryGeneralate").text(data[0].History);
                    $("#VissionGeneralate1").text(data[0].Vission);
                    $("#MissionGeneralate1").text(data[0].Mission);

                    $("#NameofFounderView").text(data[0].NameofFounder);
                    $("#NameofCoFounderView").text(data[0].NameofCoFounder);
                    $("#CongregationMottoView").text(data[0].CongregationMotto);
                    $("#CongregationCountriesofMissionView").text(data[0].CongregationCountriesofMission);

                    if (data[0].CongregationLogo == null) {
                        $("#CongregationLogoView").text("File not available");
                        $("#CongregationLogoView").removeAttr("href");
                    }
                    else {
                        $("#CongregationLogoView").text("View");
                        $("#CongregationLogoView").attr("href", "/Image/Congation/" + data[0].CongregationLogo);
                    }
                    if (data[0].File == null) {
                        $("#FileGeneralate").text("File not available");
                        $("#FileGeneralate").removeAttr("href");
                    }
                    else {
                        $("#FileGeneralate").text("View");
                        $("#FileGeneralate").attr("href", "/Image/Congation/" + data[0].File);
                    }
                },
                error: function (er) {
                }
            });
        }
    </script>

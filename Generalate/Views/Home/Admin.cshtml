<ul class="breadcrumb" style="margin-top:80px;">
    @*<b>Select Language :</b>*@
    @{
        string engRad = string.Empty;
        string frachRad = string.Empty;
        string SpanishRad = string.Empty;
        string ItalyRad = string.Empty;
        string GermanRad = string.Empty;

        if (ViewBag.ActiveLanguage == "English")
        {
            engRad = "checked";
        }
        else if (ViewBag.ActiveLanguage == "French")
        {
            frachRad = "checked";
        }
        else if (ViewBag.ActiveLanguage == "Spanish")
        {
            SpanishRad = "checked";
        }
        else if (ViewBag.ActiveLanguage == "Italy")
        {
            ItalyRad = "checked";
        }
        else if (ViewBag.ActiveLanguage == "German")
        {
            GermanRad = "checked";

        }


        if (Convert.ToString(Session["CurrentLang"]) == "EN")
        {
            engRad = "checked";
        }
        else if (Convert.ToString(Session["CurrentLang"]) == "FN")
        {
            frachRad = "checked";

        }
        else if (Convert.ToString(Session["CurrentLang"]) == "SP")
        {
            SpanishRad = "checked";
        }
        else if (Convert.ToString(Session["CurrentLang"]) == "IT")
        {
            ItalyRad = "checked";
        }
        else if (Convert.ToString(Session["CurrentLang"]) == "GR")
        {
            GermanRad = "checked";
        }




    }


    

    <li>
        <label><input value="EN" type="radio" id="englang" @engRad name="Spare3"><b> English</b></label>
    </li>
    <li>
        <label><input value="FN" type="radio" id="frlang" @frachRad name="Spare3"><b> French</b></label>
    </li>
    <li>
        <label><input value="SP" type="radio" id="frlang" @SpanishRad name="Spare3"><b> Spanish</b></label>
    </li>
    <li>
        <label><input value="IT" type="radio" id="frlang" @ItalyRad name="Spare3"><b> Italy</b></label>
    </li>
    <li>
        <label><input value="GR" type="radio" id="frlang" @GermanRad name="Spare3"><b> German</b></label>
    </li>
    <li style="display:none">
        <label><input value="@HttpContext.Current.Session["CurrentLang"]" type="text" id="Language"/></label>
    </li>
   
</ul>


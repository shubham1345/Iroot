    <script>
        $(document).ready(function () {
            GetGrid();
            $("#checkedAll").change(function () {
                if (this.checked) {
                    $(".checkSingle").each(function () {
                        this.checked = true;
                    });
                } else {
                    $(".checkSingle").each(function () {
                        this.checked = false;
                    });
                }
            });
            //var iscode = '@TempData["OperationData"]';
            var iscode = $.cookie("iscode");
            //alert(iscode);
            if (iscode == '204') {
                $("#mymessage").html("<b>Some Error Occured !</b>");
                $("#mymessage").css("color", "red");
                $('#myalertbox').modal({
                    backdrop: 'static'
                });
                $("#myalertbox").modal("show");
                $.cookie('iscode', null, { path: '/' })
            }
            if (iscode == '200') {
                $("#mymessage").html("<b>Data saved successfully !</b>");
                $("#mymessage").css("color", "green");
                $('#myalertbox').modal({
                    backdrop: 'static'
                });
                $("#myalertbox").modal("show");
                $.cookie('iscode', null, { path: '/' })
            }
            if (iscode == '201') {
                $("#mymessage").html("<b>Data updated successfully !</b>");
                $("#mymessage").css("color", "green");
                $('#myalertbox').modal({
                    backdrop: 'static'
                });
                $("#myalertbox").modal("show");
                $.cookie('iscode', null, { path: '/' })
            }
            if (iscode == '202') {
                $("#mymessage").html("<b>Data deleted successfully !</b>");
                $("#mymessage").css("color", "green");
                $('#myalertbox').modal({
                    backdrop: 'static'
                });
                $("#myalertbox").modal("show");
                $.cookie('iscode', null, { path: '/' })
            }

        });
        $("#checkbox-grid-1").on('click', function () {
            if ($('input#checkbox-grid-1').prop('checked')) {
                $(".hide-1").show();
            }
            else {
                $(".hide-1").hide();
            }
        });
        $("#checkbox-grid-2").on('click', function () {
            if ($('input#checkbox-grid-2').prop('checked')) {
                $(".hide-2").show();
            }
            else {
                $(".hide-2").hide();
            }
        });
        $("#checkbox-grid-3").on('click', function () {
            if ($('input#checkbox-grid-3').prop('checked')) {
                $(".hide-3").show();
            }
            else {
                $(".hide-3").hide();
            }
        });
        $('.numberonly').keypress(function (e) {

            var charCode = (e.which) ? e.which : event.keyCode

            if (String.fromCharCode(charCode).match(/[^0-9]/g))

                return false;

        });

        $("#circileimage").mouseover(function () {
            $("#circileimage").text("Back");
        });

        $("#circileimage").mouseout(function () {
            $("#circileimage").text('');
        });

        $("#StartingId").on('focusout', function () {
            var firstItem = $("#StartingId").val();
            var lastItem = $("#LastId").val();
            if (firstItem != '' && lastItem != '') {
                GetGrid();
            }
        });

        $("#LastId").on('focusout', function () {
            var firstItem = $("#StartingId").val();
            var lastItem = $("#LastId").val();
            if (firstItem != '' && lastItem != '') {
                GetGrid();
            }
        });
        $("#SearchTxt").on('keyup paste', function () {
            GetGrid();
        });

        $("#previousData").on('click', function () {
            var firstItem = $("#StartingId").val();
            var lastItem = $("#LastId").val();

            if (firstItem != 1) {
                $("#StartingId").val(firstItem * 1 - 10);
                $("#LastId").val(lastItem * 1 - 10);

                if ($("#example").is(":visible")) {
                    GetGrid();
                }
                //if ($("#MembersViewList").is(":visible")) {
                //    GetMembersViewList();
                //}

            }

        });

        $("#nextData").on('click', function () {
            var firstItem = $("#StartingId").val();
            var lastItem = $("#LastId").val();

            alert(lastItem);
            var totalcount = $("#totalcount").text();
            alert(totalcount);
            if (lastItem <= totalcount) {
                $("#StartingId").val(firstItem * 1 + 10);
                //var sid = $("#StartingId").val(firstItem * 1 + 10);
                $("#LastId").val(lastItem * 1 + 10);
                //alert($("#StartingId").val(firstItem * 1 + 10));

                if ($("#example").is(":visible")) {
                    GetGrid();
                }
                //if ($("#MembersViewList").is(":visible")) {
                // s   GetMembersViewList();
                //}
            }

        });



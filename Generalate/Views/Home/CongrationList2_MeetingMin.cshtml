                error: function (er) {
                }
            });
        }
        function GetCongrationDataById(id) {
            $.ajax({
                url: "/Home/GetCongrationDataById?id=" + id,
                type: "GET",
                contentType: "application/json;charset=utf-8",
                dataType: "json",
                success: function (result) {
                    if (result != null) {
                        $("#Tital").val(result.Title);
                        $("#id").val(result.id);
                        $("#Date").val(result.Date);
                        $("#Description").val(result.Description);
                        $("#CongrationId").val(result.CongrationId);
                        $("#conglistFile").text(result.File);
                    }
                },
                error: function (errormessage) {
                    alert(errormessage.responseText);

                }
            });
        }

        $(".DeleteCongDataMM").on('click', function () {
            var id = $(this).attr("data-val");
            if (confirm("Do you want to delete ?")) {
                window.location.href = "/Home/DeleteCongData12?id=" + id;
            }
        });
    </script>

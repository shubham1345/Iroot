        function GetByIdLD(id) {
            $.ajax({
                url: "/IdGenerate/GetLandDocumentByProvince?id=" + id,
                type: "GET",
                contentType: "application/json;charset=UTF-8",
                dataType: "json",
                success: function (result) {
                    //debugger;
                    $('#RD_' + result.LandType).prop('checked', true);
                    if (result.LandType == '1') {
                        $("#PCIId1").prop("disabled", false);
                        $("#PCIId2").prop("disabled", true);
                        $("#PCIId3").prop("disabled", true);
                        $('#div_drpParish').show();
                        $('#div_drpCong').hide();
                        $('#div_drpInst').hide();
                    }
                    else if (result.LandType == '2') {
                        $("#PCIId1").prop("disabled", true);
                        $("#PCIId2").prop("disabled", false);
                        $("#PCIId3").prop("disabled", true);
                        $('#div_drpParish').hide();
                        $('#div_drpCong').show();
                        $('#div_drpInst').hide();
                    }
                    else if (result.LandType == '3') {
                        $("#PCIId1").prop("disabled", true);
                        $("#PCIId2").prop("disabled", true);
                        $("#PCIId3").prop("disabled", false);
                        $('#div_drpParish').hide();
                        $('#div_drpCong').hide();
                        $('#div_drpInst').show();
                    }
                    $("#PCIId" + result.LandType).val(result.PCIId);
                    $("#LDID").val(result.Id);
                    $("#DocumentType").val(result.DocumentType);
                    $("#DateByLD").val(result.Date);
                    $("#TitleByLD").val(result.Title);
                    $("#FileLDspan").text(result.File);
                },
                error: function (errormessage) {
                    alert(errormessage.responseText);
                }
            }); return false;
        }

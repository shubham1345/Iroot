                    $("#ProvinceHistoryProvince").text(data[0].History);
                    $("#VissionProvince").text(data[0].Vission);
                    $("#MissionProvince").text(data[0].Mission);
                    $("#ProvinceMottoView").text(data[0].ProvinceMotto);
                    $("#ProvinceCountriesofMissionView").text(data[0].ProvinceCountriesofMission);
                    if (data[0].File == null) {
                        $("#FileProvince").text("File not available");
                        $("#FileProvince").removeAttr("href");
                    }
                    else {
                        $("#FileProvince").text("View");
                        $("#FileProvince").attr("href", "/Image/Province/" + data[0].File);

        function BindOrUnBindFilters(id) {
            //alert(id);
            //alert($("#SelectedGroups").html());
            $("#SearchTxt").css('width', '50%');
            $("#divSelectedGroups").show();

            if ($("#SelectedGroups").html() == '' || $("#SelectedGroups").html() == null || $("#SelectedGroups").html() == undefined) {
                $("#SelectedGroups").append("<span id='span-"+id+"'>" + id + "</span>");
            }
            else {
                var isEx = $("#SelectedGroups").html();
                if (isEx.includes("span-" + id)) {
                    //$("#span-" + id).remove();
                    $('span[id^="span-' + id + '"]').remove();
                }
                else {
                $("#SelectedGroups").append("<span id='span-"+id+"'>&gt;"+id+"</span>");
                }
            }
            if ($("#SelectedGroups").html() == '') {
                $("#SelectedGroups").empty();
                $("#divSelectedGroups").hide();
                $("#SearchTxt").css('width', '100%');
            }
            //($("#filter-"+id+" span").text("kjhbdscguysd"));
        }
        function GetGrid() {
            var firstItem = $("#StartingId").val();
            var lastItem = $("#LastId").val();
            var SearchTxt = $("#SearchTxt").val();
            var FromDate = $("#FromDate12").val();
            var ToDate = $("#ToDate12").val();
            var FilterFor = $("#filterVal").val();

            //alert(firstItem);
            jQuery.ajax({
                url: '/Home/GetGridCongCouncil',
                type: "POST",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify({ FilterFor: FilterFor, FromDate: FromDate, ToDate: ToDate, firstItem: firstItem, lastItem: lastItem, SearchTxt: SearchTxt }),
                success: function (result) {

                    $("#PartialTable").empty();
                    $("#PartialTable").html(result);
                    $('.notdivhide').prop("checked", true);

                },
                error: function (err) {
                    alert(err.statusText);
                }
            });
        }

@model Generalate.Models.tbl_SecularEducation

@{
    Layout = null;
}
<head>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/bootstrap/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="~/bootstrap/css/bootstrap.min.css">

    <script type="text/javascript">
        $(document).ready(function () {

            $.getJSON("GetAll",
            function (json) {
                var tr;

                //Append each row to html table
                for (var i = 0; i < json.length; i++) {
                    tr = $('<tr />');
                    tr.append("<td hidden='hidden'>" + json[i].SecularId + "</td>");
                    // tr.append('<td><a href=ViewProfile/' + json[i].SecularId + '>' + json[i].MemberID + '</a></td>');
                    tr.append("<td>" + json[i].Name + "</td>");
                    tr.append("<td>" + json[i].Spare1 + "</td>");
                    tr.append("<td>" + json[i].DegreeName + "</td>");
                    tr.append("<td>" + json[i].FromDate + "</td>");
                    tr.append("<td>" + json[i].ToDate + "</td>");
                    tr.append("<td>" + json[i].University + "</td>");
                    tr.append("<td hidden='hidden'>" + json[i].Address + "</td>");
                    tr.append("<td>" + json[i].ClassObtained + "</td>");
                    tr.append("<td>" + json[i].Medium + "</td>");
                    $('table').append(tr);

                }
            });
        });

    </script>

</head>


<div class="form-horizontal">
    <div class="container">
        <div class="form-horizontal">
            <br />
            <table class="table table-striped  table-hover" id="EmpInfo">
                <thead>
                    <tr>
                        <th hidden="hidden"> Id </th>
                        <th> Sur Name </th>
                        <th> Name </th>
                        <th> DegreeName </th>
                        <th> FromDate </th>
                        <th> ToDate </th>
                        <th> University </th>
                        <th hidden="hidden"> Address </th>
                        <th> ClassObtained </th>
                        <th> Medium </th>
                       
                    </tr>
                </thead>
                <tbody class="tbody"> </tbody>
            </table>
        </div>
    </div>
</div>
            if (arr.length == 1) {
                Province = data;
            }

            jQuery.ajax({
                url: '/Community/CommunityList?Provincename=' + Province,
                type: 'POST',
                contentType: "application/json;charset=utf-8",
                success: function (result) {
                    $("#listCommunity").empty();
                    $("#listCommunity").html(result);
                    $("#CommunityList").modal('show');
                }
            });

        });


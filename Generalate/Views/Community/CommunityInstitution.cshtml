            $("#ActiveBtn").css('color', 'white');
            $("#ArchieveBtn").css('color', 'black');
            $("#ActiveBtn_CSS").css('background-color', '#29b2e1');
            $("#ArchieveBtn_CSS").css('background-color', 'white');
            $('#filterData').val('');
            $("#example").show();
            $("#statiscs").hide();
            $("#MembersViewList").hide();
            $("#formation").hide();
            $(".o_list_button_add").show();
            GetGrid();
        });


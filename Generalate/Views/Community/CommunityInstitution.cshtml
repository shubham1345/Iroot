        $(".o_cp_switch_graph").on('click', function () {
            $("#example").hide();
            $("#statiscs").show();
            $("#MembersViewList").hide();
            $("#formation").show();
            $(".o_list_button_add").hide();
            $(this).siblings().removeClass('active');
            $(this).addClass('active');
        });
        $(".o_cp_switch_list").on('click', function () {
            $("#example").show();
            $("#statiscs").hide();
            $("#MembersViewList").hide();
            $("#formation").hide();
            $(".o_list_button_add").show();
            $(this).siblings().removeClass('active');
            $(this).addClass('active');
        });
        $("#previousData").on('click', function () {
            var firstItem = $("#StartingId").val();
            var lastItem = $("#LastId").val();

            if (firstItem != 1) {
                $("#StartingId").val(firstItem * 1 - 10);
                $("#LastId").val(lastItem * 1 - 10);
                GetGrid();
            }

        });
        $("#nextData").on('click', function () {
            var firstItem = $("#StartingId").val();
            var lastItem = $("#LastId").val();
            //alert(lastItem);
            var totalcount = $("#totalcount").text();
            if (parseInt(lastItem) <= parseInt(totalcount)) {
                $("#StartingId").val(parseInt(firstItem) + 10);
                $("#LastId").val(parseInt(lastItem) + 10);
                //alert($("#LastId").val());
                GetGrid();
            }

        });
        function GetStatics(fromAge = '', toAge = '', fromation = '', GroupByData = '') {
            jQuery.ajax({
                url: '/Community/GetStatisticCommunity',
                type: "POST",
                contentType: "application/json; charset=utf-8",

                success: function (result) {
                    $("#statiscs").empty();
                    $("#statiscs").html(result);
                    $("#spinner-border").hide();
                },
                error: function (err) {
                    alert(err.statusText);
                    $("#spinner-border").hide();
                }
            });
        }
        function GetGrid(e = "") {
            var firstItem = $("#StartingId").val();
            var lastItem = $("#LastId").val();
            var SearchTxt = $("#SearchTxt").val();
            var archieve = $('#filterData').val();
            //if (e != "") {
            //    archieve = "archieve";
            //}
            //alert(lastItem);
            jQuery.ajax({
                url: '/Community/GetGrid',
                type: "POST",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify({ name: 'Deep', firstItem: firstItem, lastItem: lastItem, SearchTxt: SearchTxt, Archieve: archieve }),
                success: function (result) {

                    $("#PartialTable").empty();
                    $("#PartialTable").html(result);
                    //alert($('#DynamicTotalCount').val());
                    $('#totalcount').html($('#DynamicTotalCount').val());
                    //$('#example').DataTable({
                    //    dom: 'Bfrtip',
                    //    buttons: [
                    //        'columnsToggle'
                    //    ],
                    //    searching: false,

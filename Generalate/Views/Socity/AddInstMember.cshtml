@model Generalate.Models.tbl_InstitutionFinal
@{
    ViewBag.Title = "AddInstMember";

}


<ul class="breadcrumb">
    <li><a href="#">Home</a></li>
    <li><a href="index.html">AddMember</a></li>
</ul>

<div class="page-content-wrap">
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-primary">
                <div class="panel-heading" style="background-color:#29b2e1">Add New Member</div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-12">
                            <!-- Horizontal Form -->
                            <div class="box box-info">
                                <div class="box-header with-border">
                                    <h3 class="box-title">Member Information</h3>
                                </div>
                                <form class="form-horizontal" action="/Member/AddMember" method="post">
                                    <div class="box-body">

                                        <div class="form-group">
                                            <label for="inputPassword3" class="col-sm-2 control-label">Member Id</label>
                                            <div class="col-sm-6">
                                                <input type="text" class="form-control" name="MemberId" id="MemberId" placeholder="Member Id" readonly>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label for="inputEmail3" class="col-sm-2 control-label">Institution Name</label>
                                            <div class="col-sm-6">
                                                <input type="text" class="form-control" name="Name" id="instName" placeholder="Institution Name" required>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label for="inputPassword3" class="col-sm-2 control-label">Institution Type</label>
                                            <div class="col-sm-6">
                                                <input type="text" class="form-control" name="SirName" id="insttype" required placeholder="Institution Type">
                                            </div>
                                        </div>



                                        <div class="form-group">
                                            <div class="col-sm-8">
                                             
                                                <button type="button" class="btn btn-primary" id="btnAdd1" onclick="return AddMember1();"    style="background-color:#001f6b">Add</button>
                                            </div>
                                        </div>


                                    </div>
                                </form>
                            </div>
                            <!-- /.box -->
                            <!-- general form elements disabled -->
                            <!-- /.box -->
                        </div>

                    </div>


                </div>
            </div>
        </div>
    </div>
</div>


@section scripts{
    <script src="~/Scripts/additional/InstMemberAdd.js"></script>
    <script>
        AutoGeneratreMemberId();

        function AutoGeneratreMemberId() {
            
            var membrid;
            $.ajax({
                url: "/Socity/GetAutoGeneratedMemberId",
                type: "POST",
                contentType: "application/json;charset=utf-8",
                //data : { "DlistName"},
                dataType: "json",
                success: function (result) {
                    // result = "HSM" + result;
                    result = "SCB" + result;
                    $("#MemberId").val(result);
                },
                error: function (errormessage) {
                    alert(errormessage.responseText);
                }
            });
        }
    </script>


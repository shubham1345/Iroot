@using (Html.BeginForm("User_Travel", "Travel", FormMethod.Post, new { enctype = "multipart/form-data", @id = "TravelForm" }))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.TravelId, new { htmlAttributes = new { @class = "form-control", Id = "TravelId" } })

        <div class="" style="margin-left:30px; margin-right:30px;">
            <div class="form-horizontal">
                <br />
                <div id="Documentdialouge"></div>
         
                <div class="panel-group">
                    <div class="panel panel-primary" style="margin-top:10px; background-color:#F4ECF7">


                        <div class="tab panel-heading">
                            <a style="color:white;">Travel Record</a>
                        </div>

                        <div class="" style="margin-left:20px; margin-right:20px;">
                            <div class="form-horizontal">
                                <br />
                                <table class="table table-striped  table-hover" id="EmpInfo" style="border:solid; border-color:#044244">
                                    <thead>
                                        <tr style="background-color:#044244; color:white">
                                            <th hidden="hidden"> Id </th>
                                            <th> Name </th>
                                            <th> Member ID </th>

                                            <th> Action </th>
                                        </tr>
                                    </thead>
                                    <tbody class="tbody"> </tbody>

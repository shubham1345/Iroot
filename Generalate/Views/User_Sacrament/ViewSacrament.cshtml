@model Generalate.Models.tbl_Entry
@{
    ViewBag.Title = "ViewSacrament";
    Layout = "~/Views/UserModule/_LayoutUser.cshtml";
}

<head>
    <script src="~/Scripts/additional/User_Sacraments.js"></script>
</head>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(model => model.EntryId, new { htmlAttributes = new { @class = "form-control" } })
    <div class="form-horizontal">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="" style="margin-left:20px; margin-right:20px;">
            <div class="form-horizontal">
                <br />
                @*<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#myModal" onclick="clearTextBox();">Add </button>*@

                <div class="panel-group">
                    <div class="panel panel-primary" style="margin-top:10px; background-color:#F4ECF7">
                        <div class="tab panel-heading">
                            <a style="color:white;">Sacraments</a>
                        </div>

                        <div class="" style="margin-left:20px; margin-right:20px;">
                            <div class="form-horizontal">
                                <br />
                                <table class="table table-striped  table-hover" id="EmpInfo" style="border:solid; border-color:#044244">
                                    <thead>
                                        <tr style="background-color:#044244; color:white">
                                            <th hidden="hidden"> Id </th>
                                            <th> Member ID </th>
                                            <th> Name </th>                                         
                                            <th> Action </th>
                                        </tr>
                                    </thead>
                                    <tbody class="tbody"> </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
